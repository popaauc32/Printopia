import numpy as np
import matplotlib.pyplot as plt
b=3.14
def f(x):
    return x ** 2 - 4 * x + 3
x = np.linspace(-b/-2+ 0.01, b/6 - 0.01, 1000)

y= f(x)

plt.plot(x, y, label= "f(x) = x^2 - 4x + 3")
plt.axhline(0, color = "black" , linewidth =0.50)

plt.grid(True)
plt.legend()
plt.title("f(x) = x^2 - 4x + 3")
plt.show()



